<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="hospital_a4_portrait" model="report.paperformat">
            <field name="name">قالب A4 عمودی</field>
            <field name="default">True</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">15</field>
            <field name="orientation">Portrait</field>
            <field name="header_spacing">0</field>
            <field name="format">A4</field>
        </record>
        <record id="hospital_a4_landscape" model="report.paperformat">
            <field name="name">قالب A4 افقی</field>
            <field name="default">True</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">15</field>
            <field name="orientation">Landscape</field>
            <field name="header_spacing">0</field>
            <field name="format">A4</field>
        </record>
    </data>
</odoo>